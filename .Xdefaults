

!-------------------------------------------
! XFT Settings gre
     
Xft.antialias:  true 
Xft.autohint:   false 
Xft.dpi:        96
Xft.hinting:    true 
Xft.hintstyle:  hintfull 
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb

!-------------------------------------------
! URXVT - Settings


URxvt*termane: urxvt
URxvt*depth:                        32
URxvt*dynamicColors:                on
URxvt*urgentOnBell:                 true
URxvt*borderLess:                   false
URxvt*scrollBar:                    false
URxvt.internalBorder: 50
URxvt.externalBorder: 0

URxvt.externalBorderColor: #000000

/* URxvt*font: xft:Inconsolata:Regular:pixelsize=14 */

URxvt*font: xft:Hack:Regular:pixelsize=12

URxvt*shading: 100
URxvt*fading: 20
!--------------------------------------------
! URxvt Colors

URxvt.colorIT:                       #b291af
URxvt.colorBD:                       #6f6776
URxvt.colorUL:                       #e9cfd3


/* urxvt*background: rgba:1200/1200/1200/E800 */
! urxvt*background: rgba:2500/2300/1700/E800
urxvt*background: rgba:3100/2500/2500/FB00


! *background: #32302f */

*foreground: #ffe9b3
!*foreground: #ebdbb2

